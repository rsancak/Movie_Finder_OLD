{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "quotes": [0, "single"],
        "import/prefer-default-export": "off",
        "no-unused-vars": [ "error", { "varsIgnorePattern": "$" } ]
    }
}
